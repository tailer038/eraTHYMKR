	<X>
		REPEAT 10
			CALL REPEAT_GOGUMA
			PRINTFORML COUNT ={COUNT}
		REND

		@(LOCAL:99)_GOGUMA
		REPEAT 5
			PRINTFORML 고구마
		REND
