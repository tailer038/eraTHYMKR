ELSEIF TALENT:150 && MARK:9 < 3
	LOCAL:1 = 1
;淫乱
ELSEIF TALENT:170
	LOCAL:1 = 2
;反発刻印Lv3
ELSEIF MARK:9 == 3
	LOCAL:1 = 3
;それ以外
ELSE
	LOCAL:1 = 5
ENDIF

;退避した対象を元に戻す
TARGET = LOCAL
RETURN LOCAL:1


