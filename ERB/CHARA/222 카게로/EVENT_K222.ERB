;-------------------------------------------------
;ローション CFLAG:7170
;-------------------------------------------------
@KOJO_222_TRAIN_MESSAGE_F_COM_60

@KOJO_222_TRAIN_MESSAGE_S_COM_60
	CALL K222_KOJO_MESSAGE_B
	SIF CFLAG:6951 == 1
		RETURN 0

	;初めて
	IF CFLAG:7170 == 0
		;연모
		IF TALENT:연모 == 1
			PRINTFORMW 「これで、準備万端ね」
			PRINTFORMW 影狼は熱い視線をあなたに送ってきた
		;それ以外
		ELSE
			PRINTFORMW 「やだ、そんなの…毛が汚れるじゃない…」
			PRINTFORMW 嫌だ嫌だと懇願するが、無視してローションを塗りたくった
		ENDIF
			CFLAG:7170 = 1
	;二回目以降
	ELSE
		;연모
		IF TALENT:연모 == 1
			;乱数分岐:2
			IF RAND:2 == 0
				PRINTFORMW 「%CSTR:90%、食べごろだよぉ」
				PRINTFORMW 待ちきれないと言わんばかりに影狼は蠱惑的なポーズをしてみせた
			;その他の場合
			ELSE
				PRINTFORMW 「%CSTR:90%が、塗ってくれない？」
				PRINTFORMW 上目遣いでねだる影狼の全身に、ローションを塗ってやった
				PRINTFORMW 「あっ、あぁん…えへへ、ありがとう」
			ENDIF
		;굴복각인Lv2以上
		ELSEIF MARK:굴복각인 >= 2
			;乱数分岐:2
			IF RAND:2 == 0
				PRINTFORMW 「んんっ…ベトベト……」
				PRINTFORMW 嫌がってはいるが抵抗はせず大人しく塗られている
			;その他の場合
			ELSE
				PRINTFORMW 「…その、この後って……」
				PRINTFORMW ローションを塗り終わると、影狼はこの後の事を考えもじもじしている
			ENDIF
		;それ以外
		ELSE
			;乱数分岐:2
			IF RAND:2 == 0
				PRINTFORMW 「わ、わかったから……せめて自分でやらせて……」
				PRINTFORMW 毛が汚れないように気を付けながら渋々とローションを塗り始めた
			;その他の場合
			ELSE
				PRINTFORMW 「うぅ、またベトベト…」
				PRINTFORMW ローションまみれになった影狼は嫌悪感を露わにした
			ENDIF
		ENDIF
	ENDIF

@KOJO_222_TRAIN_TEXT_COM_60

