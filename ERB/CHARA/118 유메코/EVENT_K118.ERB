@KOJO_118_TRAIN_MESSAGE_F_COM_174
	CALL K118_KOJO_MESSAGE_A
@KOJO_118_TRAIN_MESSAGE_S_COM_174
	CALL K118_KOJO_MESSAGE_B
	SIF CFLAG:6951 == 1
		RETURN 0

	;開始時
	IF TEQUIP:25 == 7
		;初めて
		IF CFLAG:7328 == 0
			;연모
			IF TALENT:연모 == 1
				PRINTFORMW 「か,浣腸ですか?」
				PRINTFORMW 「流石にそれはやった事がないです」
				PRINTFORMW 夢子は躊躇いながら%CALLNAME:MASTER%に尻を向け,少しだけ怯えた表情を浮かべた
				PRINTFORMW 「お願い,します……」
			;それ以外
			ELSE
				PRINTFORMW 「あっ,あなた何を考えているのよ!」
				PRINTFORMW 「浣腸なんて冗談じゃないわ!」
				PRINTFORMW 夢子は激しく抵抗を繰り返した
				PRINTFORMW しかし,力を封印された夢子が,%CALLNAME:MASTER%に敵う筈もなかった
			ENDIF
				CFLAG:7328 = 1
		;二回目以降
		ELSE
			;연모＋배설중독Lv3以上
			IF TALENT:연모 == 1 && ABL:배설중독 >= 3
				PRINTFORMW 「はぁ,んっ……綺麗に,して下さるんですね?」
				PRINTFORMW 「分かり,ました…….後で,たくさん,突いて下さいね?」
			;연모
			ELSEIF TALENT:연모 == 1
				PRINTFORMW 「はぐっ! お腹が,ぐるぐるしてますっ!」
			;배설중독Lv3以上
			ELSEIF ABL:배설중독 >= 3
				PRINTFORMW 「くっ……うんっ……っ!」
				PRINTFORMW アナルで感じているのか,夢子は時折体を震わせた
			;それ以外
			ELSE
				PRINTFORMW 「くっ,うっ……はぁ,っはぁ……」
				PRINTFORMW 夢子は苦しそうな声を上げ,呼吸を荒くしていった
			ENDIF
		ENDIF
	;脱着時
	ELSEIF TEQUIP:25 == 0
		;初めて
		IF CFLAG:7329 == 0
			;연모
			IF TALENT:연모 == 1
				PRINTFORMW 「あっ,取ってしまったら!」
				PRINTFORMW 「あっ,駄目っ! 出ちゃいますぅ!」
			;それ以外
			ELSE
				PRINTFORMW 「くっ,だ,駄目っ! いっ,いやぁぁ……っ!」
				PRINTFORMW 排泄をしてしまった夢子のプライドは,ズタズタに引き裂かれた
				PRINTFORMW いくら気丈に振る舞っていても,所詮はか弱い少女のようだ
			ENDIF
				CFLAG:7329 = 1
		;二回目以降
		ELSE
			;연모＋배설중독Lv3以上
			IF TALENT:연모 == 1 && ABL:배설중독 >= 3
				PRINTFORMW 「ひゃ,あっ,あぁん!」
				PRINTFORMW 強烈な快感を感じているのか,夢子は何度も体を震わせた
			;연모
			ELSEIF TALENT:연모 == 1
				PRINTFORMW 「……そんなに,楽しいですか?」
				PRINTFORMW 「私の方はすごく恥ずかしいのですが……」
			;배설중독Lv3以上
			ELSEIF ABL:배설중독 >= 3
				PRINTFORMW 「見ないで……お願いだから見ないで……」
			;それ以外
			ELSE
				PRINTFORMW 「くっ……うっ……っ!」
				PRINTFORMW 夢子は歯を食い縛り,溢れ出る屈辱を押し殺した
			ENDIF
		ENDIF
	ENDIF

